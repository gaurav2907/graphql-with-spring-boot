type Vehicle {
	id: ID!,
	type: String,
	modelCode: String,
	brandName: String,
	launchDate: String,
	status: VehicleStatus
}

type Query {
	vehicles(count: Int):[Vehicle]
	vehicle(id: ID):Vehicle
	vehicle1(brandName: String):[Vehicle]
}


type Mutation {
	createVehicle(type: String!, modelCode: String!, brandName: String!, launchDate: String, status: VehicleStatus):Vehicle
}

enum VehicleStatus {
    ACTIVE
    INACTIVE
}